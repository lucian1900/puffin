$load 'type.pbc';
$load 'int.pbc';

namespace Python [HLL] {

class bool : int {
    function bool(env) {
        self.int(env);
        self.__name__ = 'bool';
        self.__bases__.push(env.int);

        self.__dict__['__init__'] = function(obj, val) {
            obj.content = val;
        };
        self.__dict__['__repr__'] = function(obj) {
            if(obj.content)
                return 'True';
            else
                return 'False';
        };
    }
}

}
